        "babel-eslint": "^8.2.6",
        "babel-loader": "^7.1.5",
        "babel-preset-es2015": "^6.24.1",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^0.28.11",
        "eslint": "^5.6.0",
        "eslint-loader": "^2.1.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
